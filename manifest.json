{
    "manifest_version": 3,
    "name": "Canvas Pins Extension",
    "description": "Allows you to pin module items in Canvas!",
    "version": "1.0",
    "action": {
        "default_popup": "hello.html",
        "default_icon": "logo.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/module-pins.js"],
            "matches": [
                "https://pasadena.instructure.com/courses/*"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}